{
    "name": "bolt/bolt-docs",
    "description": "Documentation for Bolt",
    "homepage": "http://docs.bolt.cm",
    "keywords": ["Bolt", "Docs", "Documentation", "CMS", "Silex", "PHP", "Symfony"],
    "license": "MIT",
    "require": {
        "cocur/slugify" : "~1.0",
        "erusev/parsedown-extra": "~0.7",
        "monolog/monolog": "~1.12",
        "silex/silex": "~2.0@dev",
        "silex/web-profiler": "~2.0@dev",
        "symfony/console": "^3.0",
        "symfony/finder": "^3.0",
        "symfony/var-dumper": "^3.0",
        "symfony/web-profiler-bundle" : "^3.0",
        "symfony/yaml": "^3.0",
        "twig/twig": "~1.21"
    },
    "minimum-stability": "beta",
    "config": {
        "preferred-install": "dist"
    },
    "autoload": {
        "psr-4": {
            "Bolt\\Docs\\": "./src/"
        }
    },
    "prefer-stable": true
}
